{
    "name": "shamaseen/laravel-repositories",
    "description": "repository pattern files generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Shamaseen",
            "email": "m.shamaseen@outlook.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Shamaseen\\Repository\\RepositoryServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Shamaseen\\Repository\\Tests\\": "tests/",
            "Shamaseen\\Repository\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "shamaseen/laravel-files-generator": "^0.5",
        "laravel/framework": ">5.7"
    },
    "require-dev": {
        "doctrine/dbal": ">2.8",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.8"
    }
}
